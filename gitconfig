[user]
	name = Syknapse
	email = 29199184+Syknapse@users.noreply.github.com
[init]
	defaultBranch = main
[alias]
  st = status
  co = checkout
  # Show last commit
  last = log -1 HEAD
  # Show current user
  whoami = !git config user.name && git config user.email
# If there is a work directory take gitconfig from there.
# This allows us to have a different user in work folders
[includeIf "gitdir:~/work/"]
    path = ~/work/.gitconfig
[core]
	editor = nano
	excludesfile = /Users/sykh/.gitignore
